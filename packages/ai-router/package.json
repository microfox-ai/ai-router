{
  "name": "@microfox/ai-router",
  "version": "2.1.2",
  "description": "A Router-based Framework built on top of ai-sdk to support Multi-Agentic Workflows.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts",
    "prettier-check": "prettier --check \"./**/*.ts*\"",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./workflow": {
      "types": "./dist/workflow.d.ts",
      "import": "./dist/workflow.mjs",
      "require": "./dist/workflow.js"
    },
    "./workflow/orchestrate": {
      "types": "./dist/workflow/orchestrate.d.ts",
      "import": "./dist/workflow/orchestrate.mjs",
      "require": "./dist/workflow/orchestrate.js"
    },
    "./fs_store": {
      "types": "./dist/fs_store.d.ts",
      "import": "./dist/fs_store.mjs",
      "require": "./dist/fs_store.js"
    }
  },
  "dependencies": {
    "ai": "^5.0.51",
    "async-lock": "^1.4.1",
    "zod": "^4.1.5"
  },
  "peerDependencies": {
    "ai": ">=5.0.47",
    "workflow": "^4.0.1-beta.35"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "tsup": "^8",
    "typescript": "^5.6.3",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "@microfox/ai-router",
    "typescript",
    "api",
    "sdk"
  ]
}
