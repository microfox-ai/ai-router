{
  "name": "@microfox/ai-studio-react",
  "version": "1.0.2-beta.0",
  "description": "A TypeScript SDK for Ai Studio React.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint \"./**/*.ts*\"",
    "prettier-check": "prettier --check \"./**/*.ts*\"",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.30",
    "@microfox/ai-router": "^2.0.1-beta.0",
    "@microfox/db-upstash": "^1.1.8",
    "@microfox/tool-kit": "^1.1.22",
    "@microfox/types": "^1.0.16",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tiptap/extension-code-block": "^3.3.0",
    "@tiptap/extension-link": "^3.3.0",
    "@tiptap/extension-table": "^3.3.0",
    "@tiptap/extension-table-cell": "^3.3.0",
    "@tiptap/extension-table-header": "^3.3.0",
    "@tiptap/extension-table-row": "^3.3.0",
    "@tiptap/react": "^3.3.0",
    "@tiptap/starter-kit": "^3.3.0",
    "@upstash/redis": "^1.35.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-resizable-panels": "^3.0.5",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "remark-parse": "^11.0.0",
    "sonner": "^2.0.7",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "unified": "^11.0.5",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "latest",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^9.1.0",
    "prettier": "^3.0.0",
    "tsup": "^8",
    "typescript": "^5.6.3",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai-studio-react",
    "typescript",
    "api",
    "sdk"
  ]
}
